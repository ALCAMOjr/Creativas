{
  "canisters": {
    "Creativas_backend": {
      "candid": "src/Creativas_backend/Creativas_backend.did",
      "package": "Creativas_backend",
      "type": "rust"
    },
    "Creativas_frontend": {
      "dependencies": [
        "Creativas_backend"
      ],
      "source": [
        "src/Creativas_frontend/dist"
      ],
      "type": "assets",
      "workspace": "Creativas_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8080",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "version": 1
}