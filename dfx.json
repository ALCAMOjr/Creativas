{
  "canisters": {
    "eccomerce": {
      "type": "rust",
      "candid": "src/backend/eccomerce/eccomerce.did",
      "package": "eccomerce",
      "declarations": {
        "node_compatibility": true
      }
    },
    "nft_venture": {
      "type": "rust",
      "candid": "src/backend/nft_venture/nft_venture.did",
      "package": "nft_venture",
      "declarations": {
        "node_compatibility": true
      }
    },
    "frontend": {
      "dependencies": [
        "eccomerce",
        "nft_venture"
      ],
      "source": [
        "src/frontend/dist"
      ],
      "type": "assets",
      "workspace": "frontend"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:1200",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "version": 1
}