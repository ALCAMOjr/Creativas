{
  "canisters": {
    "eccomerce": {
      "type": "rust",
      "candid": "backend/eccomerce/eccomerce.did",
      "package": "eccomerce",
      "declarations": {
        "node_compatibility": true
      }
    },
    "nft_venture": {
      "type": "rust",
      "candid": "backend/nft_venture/nft_venture.did",
      "package": "nft_venture",
      "declarations": {
        "node_compatibility": true
      }
    },

    "assets": {
      "dependencies": [
        "eccomerce",
        "nft_venture"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:7070",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "output_env_file": ".env.local",
  "version": 1
}